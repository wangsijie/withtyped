{
  "name": "@withtyped/server",
  "version": "0.11.1",
  "author": "Gao Sun",
  "license": "MIT",
  "type": "module",
  "source": "./src/index.ts",
  "exports": {
    ".": {
      "import": "./lib/index.js"
    },
    "./lib/*": {
      "import": "./lib/*"
    },
    "./model": {
      "import": "./lib/model/index.js"
    }
  },
  "module": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "files": [
    "lib"
  ],
  "scripts": {
    "precommit": "lint-staged",
    "prepack": "pnpm build",
    "build": "rm -rf lib/ && tsc",
    "dev": "pnpm build --watch --preserveWatchOutput --incremental",
    "dev:test": "pnpm build --sourcemap && pnpm test",
    "lint": "eslint src/",
    "test": "c8 --all node --test | tap-arc",
    "test:only": "node --test"
  },
  "devDependencies": {
    "@faker-js/faker": "^7.6.0",
    "@silverhand/eslint-config": "^3.0.0",
    "@silverhand/ts-config": "^3.0.0",
    "@types/node": "^18.11.9",
    "@types/sinon": "^10.0.13",
    "@types/supertest": "^2.0.12",
    "c8": "^7.12.0",
    "eslint": "^8.36.0",
    "lint-staged": "^13.0.4",
    "openapi-schema-validator": "^12.0.2",
    "prettier": "^2.8.4",
    "sinon": "^14.0.2",
    "supertest": "^6.3.2",
    "tap-arc": "^0.3.5",
    "typescript": "^5.1.0",
    "zod": "^3.19.1"
  },
  "eslintConfig": {
    "extends": "@silverhand",
    "rules": {
      "unicorn/prevent-abbreviations": "off",
      "complexity": [
        "error",
        11
      ]
    },
    "ignorePatterns": [
      "src/openapi/openapi-types.ts"
    ]
  },
  "prettier": "@silverhand/eslint-config/.prettierrc",
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@silverhand/essentials": "^2.5.0",
    "@withtyped/shared": "workspace:*"
  },
  "peerDependencies": {
    "zod": "^3.19.1"
  }
}
